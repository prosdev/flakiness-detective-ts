{
  "name": "@flakiness-detective/core",
  "version": "0.1.1",
  "description": "Core detection engine for identifying flaky tests using AI embeddings and DBSCAN clustering",
  "keywords": [
    "testing",
    "flakiness",
    "test-automation",
    "playwright",
    "clustering",
    "dbscan",
    "ai",
    "machine-learning"
  ],
  "author": "prosdevlab",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/prosdev/flakiness-detective-ts.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/prosdev/flakiness-detective-ts/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/prosdev/flakiness-detective-ts/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@flakiness-detective/utils": "workspace:*",
    "density-clustering": "^1.3.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22"
  }
}